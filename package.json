{
  "name": "olivine-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Frontend for Olivine V4 - AI Workflow Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx vite build --outDir dist",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.1",
    "@sendgrid/mail": "^8.1.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tsndr/cloudflare-worker-jwt": "^3.1.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.17",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "date-fns": "^3.3.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pinia": "^3.0.1",
    "postcss": "^8.4.35",
    "socket.io-client": "^4.7.4",
    "tailwindcss": "^3.4.1",
    "terser": "^5.26.0",
    "uuid": "^11.1.0",
    "vite": "^5.1.6",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "vercel": "33.7.0",
    "vitest": "^1.3.1"
  }
}