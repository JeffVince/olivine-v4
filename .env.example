# API and WebSocket URLs
VITE_API_URL=http://localhost:3000/api
VITE_WS_URL=ws://localhost:3000

# App configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=Olivine V4

# Logging configuration - "debug", "info", "warn", "error"
VITE_LOG_LEVEL=debug

# AstraDB Configuration (only variables safe for client-side)
VITE_ASTRA_DB_ID=your_astra_db_id

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Server-side environment variables (for Vercel serverless functions)
# These should be set in Vercel dashboard, not in client-side code
# JWT_SECRET=your_jwt_secret
# PULSAR_API_URL=your_pulsar_endpoint
# PULSAR_API_KEY=your_pulsar_api_key
# ASTRA_DB_APPLICATION_TOKEN=your_astra_db_token
# ASTRA_DB_REGION=your_astra_db_region 